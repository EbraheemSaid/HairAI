@echo off\r\nsetlocal enabledelayedexpansion\r\n\r\necho \ud83d\udd27 Testing HairAI API Bootstrap Admin Endpoint\r\necho =============================================\r\n\r\nset BASE_URL=http://localhost:5000/api\r\n\r\nREM Test health endpoint first\r\necho 1. Testing health endpoint...\r\ncurl -s %BASE_URL%/health > health_response.json\r\nfindstr \"healthy\" health_response.json >nul\r\nif !errorlevel! equ 0 (\r\n    echo \u2705 API is running and healthy\r\n) else (\r\n    echo \u274c API is not responding\r\n    echo Make sure to:\r\n    echo 1. cd Backend\\HairAI.Api\r\n    echo 2. dotnet restore\r\n    echo 3. dotnet run\r\n    del health_response.json\r\n    exit /b 1\r\n)\r\ndel health_response.json\r\n\r\nREM Test bootstrap-admin endpoint\r\necho.\r\necho 2. Testing bootstrap-admin endpoint...\r\ncurl -s -X POST -H \"Content-Type: application/json\" %BASE_URL%/auth/bootstrap-admin > bootstrap_response.json\r\n\r\nfindstr \"\\\"success\\\":true\" bootstrap_response.json >nul\r\nif !errorlevel! equ 0 (\r\n    echo \u2705 Bootstrap admin successful\r\n    \r\n    REM Extract admin details using PowerShell\r\n    powershell -Command ^\r\n        \"$response = Get-Content bootstrap_response.json | ConvertFrom-Json; ^\r\n        Write-Host 'Response:'; ^\r\n        Write-Host '  Email: ' $response.data.email; ^\r\n        Write-Host '  User ID: ' $response.data.userId; ^\r\n        Write-Host '  Default Password: ' $response.data.defaultPassword\"\r\n    \r\n    echo.\r\n    echo \ud83c\udf89 You can now login with these credentials!\r\n) else (\r\n    echo \u274c Bootstrap admin failed\r\n    type bootstrap_response.json\r\n)\r\ndel bootstrap_response.json\r\n\r\necho.\r\necho =============================================\r\necho Bootstrap admin test completed!